Set new plymouth
	sudo update-alternatives --install /usr/share/plymouth/themes/default.plymouth default.plymouth /usr/share/plymouth/themes/mytheme/mytheme.plymouth 123
	sudo update-alternatives --config default.plymouth
	sudo update-initramfs -u
	sudo reboot
	
	
Install plank
	sudo apt-get update
	sudo apt-get install plank
	sudo apt-get remove plank
	plank --version
	plank --preferences
	
	
	
Install gnome tweak tool
	sudo apt-get install gnome-tweak-tool
	
